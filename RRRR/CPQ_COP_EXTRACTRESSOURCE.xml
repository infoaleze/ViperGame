				<nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT
   @SELECT:DIM:USER_DEF:IMPLIED:L:A.ID:ID@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.LOGIN_PCS:LOGIN_PCS@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.NOM:NOM@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.PRENOM:PRENOM@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.EMAIL:EMAIL@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.TYPE_CONTRAT:TYPE_CONTRAT@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.ACTIF:ACTIF@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.DEBUT_PRESENCE:DEBUT_PRESENCE@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.FIN_PRESENCE:FIN_PRESENCE@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.CAPACITE:CAPACITE@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.EQUIPE:EQUIPE@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.REFERENCE_ANALYTIQUE:REFERENCE_ANALYTIQUE@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.TJM:TJM@
FROM
(   
SELECT 
	R.UNIQUE_NAME AS ID,
	R.pcs_id_pcs AS LOGIN_PCS,
	R.LAST_NAME AS NOM,
	R.FIRST_NAME AS PRENOM,
	R.EMAIL AS EMAIL,
	type_contrat.description TYPE_CONTRAT,
	CASE 
		WHEN R.IS_ACTIVE=1 THEN 1
		ELSE 0
	END ACTIF,
	TO_CHAR(R.DATE_OF_HIRE,'DD/MM/YYYY') DEBUT_PRESENCE,
	TO_CHAR(R.DATE_OF_TERMINATION,'DD/MM/YYYY') FIN_PRESENCE,
	R.pcs_capacite AS CAPACITE,
	REF_OBS.OBS_TEAM_NAME	EQUIPE, 
	REF_OBS.OBS_TEAM_CODE	REFERENCE_ANALYTIQUE,	
	R.pcs_tjm AS TJM
FROM 
	ODF_RESOURCE_V2 R	
	
		left join cmn_lookups_v type_contrat on type_contrat.lookup_type = 'SRM_RESOURCE_TYPE' and type_contrat.ID = R.PERSON_TYPE and type_contrat.language_code = 'fr'
	

		left join (
			SELECT
				OA.RECORD_ID UNIT_ID,
				OBS_Team.OU_NAME OBS_TEAM_NAME,
				substring(OBS_Team.CODE from 3) OBS_TEAM_CODE
			FROM 
				PRJ_OBS_ASSOCIATIONS OA 
					inner join PRJ_OBS_UNITS OU on (OA.UNIT_ID = OU.ID)
						inner join prj_obs_types OT on (OU.type_id = OT.ID AND OT.unique_name = 'Organisation')
								
							left join (
								SELECT 	OBF.unit_id UNIT_ID, OU.unique_name CODE, OU.name OU_NAME
								FROM 
									prj_obs_units_flat OBF 
										inner join  prj_obs_units OU  on (OU.id = OBF.branch_unit_id and OU.DEPTH = 4)
								) OBS_Team ON (OU.ID = OBS_Team.UNIT_ID )	
			WHERE
				OA.TABLE_NAME = 'SRM_RESOURCES'
			) REF_OBS ON (R.ODF_PK = REF_OBS.UNIT_ID)

		inner join PRJ_OBS_ASSOCIATIONS OA ON (OA.RECORD_ID = R.ODF_PK and OA.TABLE_NAME = 'SRM_RESOURCES')			
			inner join PRJ_OBS_UNITS OU ON (OU.ID = OA.UNIT_ID)
				inner join prj_obs_types OT on (OU.type_id = OT.ID AND OT.unique_name = 'Organisation')

		
	
WHERE
	R.prISROLE = 0
AND	R.ODF_PK > 500000	
) A
WHERE
  @FILTER@]]></nsql>
