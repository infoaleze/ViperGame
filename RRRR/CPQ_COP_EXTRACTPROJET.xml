				<nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT
   @SELECT:DIM:USER_DEF:IMPLIED:L:A.ID_PROJET:ID_PROJET@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.CODE_PROJET:CODE_PROJET@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.LIBELLE_PROJET:LIBELLE_PROJET@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.CHEF_DE_PROJET:CHEF_DE_PROJET@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.ACTIVITE:ACTIVITE@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.PLAN_HORS_PLAN:PLAN_HORS_PLAN@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.OUVERT_SAISIE:OUVERT_SAISIE@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.DATE_DEBUT:DATE_DEBUT@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.DATE_FIN:DATE_FIN@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.EXCEPTION_SPIROU:EXCEPTION_SPIROU@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.ACTIF:ACTIF@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.ENVELOPPE:ENVELOPPE@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.PRIORITE:PRIORITE@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.PARTENAIRE:PARTENAIRE@
FROM (
SELECT
	P.ODF_PK       ID_PROJET,
	P.PROJECT_CODE CODE_PROJET,
	P.NAME         LIBELLE_PROJET,
	RM.pcs_id_pcs  CHEF_DE_PROJET,
	L_ACTIVITE.NAME AS ACTIVITE,
	CASE P.PCS_DLP
		WHEN 0 THEN 0
		ELSE 1
	END            PLAN_HORS_PLAN,
	CASE P.ISOPEN
		WHEN 0 THEN 0
		ELSE 1
	END            OUVERT_SAISIE,
	TO_CHAR(P.SCHEDULE_START,'DD/MM/YYYY') DATE_DEBUT,	
	TO_CHAR(P.SCHEDULE_FINISH,'DD/MM/YYYY') DATE_FIN,
	CASE P.pcs_spirou
		WHEN 0 THEN 0
		ELSE 1
	END EXCEPTION_SPIROU,
	CASE P.IS_ACTIVE
		WHEN 0 THEN 0
		ELSE 1
	END ACTIF,
	L_ENVELOPPE.name ENVELOPPE,
	L_PRIORITE.name PRIORITE,
	L_PARTENAIRE.name PARTENAIRE
	
FROM
