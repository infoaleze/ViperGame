				<nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT
   @SELECT:DIM:USER_DEF:IMPLIED:L:A.UNIQUE_ID:UNIQUE_ID@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.INV_CODE:CODE_PROJET@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.TEAM_CODE:CODE_RESOURCE@,   
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.TEAM_NAME:NOM_RESOURCE@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.DT_REF_MIN:DEBUT@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.DT_REF_MAX:FIN@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.DT_CREATE:TACHE_CREE@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.DT_UPD:TACHE_MODIFIEE@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.allocM:AFFECTE_M@,
   @SELECT:DIM_PROP:USER_DEF:IMPLIED:L:A.allocSum:AFFECTE_TT@
FROM
(
SELECT
    I.CODE || TD.CODE || to_char(REF_DATE.START_DATE, 'MMYYYY') UNIQUE_ID,
    I.CODE INV_CODE,
    TD.CODE TEAM_CODE,
    TD.NAME TEAM_NAME,
    TO_CHAR(REF_DATE.START_DATE, '01/MM/YYYY') DT_REF_MIN,
    TO_CHAR(REF_DATE.FINISH_DATE-1, 'DD/MM/YYYY') DT_REF_MAX,
    TO_CHAR(T.CREATED_DATE, 'DD/MM/YYYY') DT_CREATE,
    TO_CHAR(T.LAST_UPDATED_DATE, 'DD/MM/YYYY') DT_UPD,
    NK_SUM_FCT(T.odf_nk_odf_pralloccurve, REF_DATE.START_DATE, REF_DATE.FINISH_DATE) /28800 AS allocM,
    T.prAllocSum / 28800 AS allocSum

FROM
    INV_INVESTMENTS I
        INNER JOIN PRTeam T ON (T.prProjectID = I.ID)
            INNER JOIN SRM_RESOURCES RT on (RT.ID = T.prResourceID)
                INNER JOIN TEAM_DEFINITIONS TD on (TD.ID = RT.DEFINED_TEAM_ID)

    CROSS JOIN (SELECT START_DATE, FINISH_DATE 
        FROM unnest( PFM_DEFAULT_CALENDAR_FCT( CAST(DATE_TRUNC('year',CURRENT_DATE-730) AS timestamp) , 
                                                'MONTH',60
                                                )
                    )
        ) REF_DATE
WHERE
    I.IS_ACTIVE = 1
AND T.prAllocSum > 0
) A
WHERE
  A.allocM > 0
AND @FILTER@]]></nsql>
